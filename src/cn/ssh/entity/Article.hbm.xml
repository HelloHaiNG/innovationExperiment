<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="cn.ssh.entity.Article" table="t_article" lazy="false">
     <id name="artId" column="artId">
         <generator class="native"></generator>
     </id>
     <property name="artAuthor" column="artAuthor"></property>
     <property name="artTitle" column="artTitle"></property>
     <property name="artContent" column="artContent"></property>
     <property name="artResult" column="artResult"></property>
     <property name="masNumber" column="masNumber"></property>
   </class>
</hibernate-mapping>

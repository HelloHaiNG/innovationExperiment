<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="cn.ssh.entity.Love" table="t_love" lazy="false">
     <id name="loveId" column="loveId">
         <generator class="native"></generator>
     </id>
     <property name="fromStuName" column="fromStuName"></property>
     <property name="toName" column="toName"></property>
     <property name="loveContent" column="loveContent"></property>
     <property name="loveResult" column="loveResult"></property>
     <property name="loveReason" column="loveReason"></property>
     <property name="stuNumber" column="stuNumber"></property>
   </class>
</hibernate-mapping>
